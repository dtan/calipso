        "copyFrom":"default",
        "sections": {
          "body": { "template": "body.html" }
        }
      }
    },    
    "quickstartLanding": {
      "layout": {
        "copyFrom":"default",
        "sections": {
          "body": { "template": "body.html" }
        }
      }
